<view><uni-steps vue-id="1" options="{{[{title:'输入文本'},{title:'选择模板'},{title:'生成导出'}]}}" active="{{0}}" bind:__l="__l"></uni-steps><block wx:if="{{pageType=='catalog'}}"><view><block wx:for="{{content.catalog.titles.value}}" wx:for-item="item" wx:for-index="index"><view><block wx:if="{{item!='-'}}"><view><view class="inputBox"><block wx:if="{{index==0}}"><view><uni-icons class="addIcon" vue-id="{{'2-'+index}}" type="plus" size="30" data-event-opts="{{[['^tap',[['addOneMenuItem',[index]]]]]}}" bind:tap="__e" bind:__l="__l"></uni-icons></view></block><block wx:else><view><uni-icons class="addIcon" vue-id="{{'3-'+index}}" type="minus" size="30" data-event-opts="{{[['^tap',[['deleteOneMenuItem',[index]]]]]}}" bind:tap="__e" bind:__l="__l"></uni-icons></view></block><input class="inputContent" placeholder="{{'请输入目录'+(index+1)}}" data-event-opts="{{[['input',[['inputOneMenuItem',['$event',index]]]]]}}" value="{{content.catalog.titles.value[index]}}" bindinput="__e"/><view class="divLine"></view></view></view></block></view></block></view></block><block wx:else><view><block wx:for="{{content[pageType]}}" wx:for-item="obj" wx:for-index="key" wx:key="tip"><view class="inputBox"><view>{{obj.tip}}</view><input class="inputContent" placeholder="{{'请输入'+obj.tip}}" data-event-opts="{{[['input',[['changeContent',['$event',key]]]]]}}" value="{{content[pageType][key].value}}" bindinput="__e"/><view class="divLine"></view></view></block></view></block><button class="doneButton" type="default" plain="true" loading="{{buttonLoading}}" data-event-opts="{{[['tap',[['previewThis',['$event']]]]]}}" bindtap="__e">完 成</button></view>