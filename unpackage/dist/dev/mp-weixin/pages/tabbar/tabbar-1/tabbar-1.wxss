.userinfo-avatar {  
  overflow: hidden;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  border: 1px solid #fff;
  box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.1);
  margin-left: 20rpx;
}
/* 导航栏 */
.status_bar {
  height: 25px;
  width: 100%;
}
/* 为了适配自定义的导航栏 */
.content {
	margin-top: 50rpx;
}

